
clear

d = 14.0;
rho = 1;

v0 = 2.3810e-04;
targetV = v0*10.^([0:10]/2);

v = targetV(1:2);

eta = [1/3, 1/3];

re = d*v./eta;

dc1 = drag_coeff(re);
dc2 = drag_coeff_2(re);

f = [0.009275, 0.028909];

r = d/2;
dclb = 2.*f./(rho*v.*v.*pi*r*r);

function dc = drag_coeff(re)

dc = (24./re).*(1+0.1806*re.^0.6459) + 0.4251./(1+6880.95./re);

end

function dc = drag_coeff_2(re)

dc = (24./re).*(1+0.27.*re).^0.43 + 0.47.*(1-exp(-0.04.*re.^0.38));

end

